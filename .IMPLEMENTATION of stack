class Stack {
    int[] arr;
    int top;
    int capacity;

    public Stack(int n) {
        arr = new int[n];
        capacity = n;
        top = -1;
    }

    // 1. push(x)
    public void push(int x) {
        if (isFull()) return;
        arr[++top] = x;
    }

    // 2. pop()
    public void pop() {
        if (isEmpty()) return;
        top--;
    }

    // 3. peek()
    public int peek() {
        if (isEmpty()) return -1;
        return arr[top];
    }

    // 4. isEmpty()
    public boolean isEmpty() {
        return top == -1;
    }

    // 5. isFull()
    public boolean isFull() {
        return top == capacity - 1;
    }
}
